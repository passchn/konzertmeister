{
  "name": "passchn/konzertmeister",
  "description": "Konzertmeister Integration for PHP",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "passchn",
      "email": "dev@pascalschneider.eu"
    }
  ],
  "require": {
    "php": "^8.1",
    "guzzlehttp/guzzle": "^7.0",
    "nette/http": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10"
  },
  "autoload": {
    "psr-4": {
      "Passchn\\Konzertmeister\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Passchn\\Konzertmeister\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "/Applications/MAMP/bin/php/php8.1.1/bin/php ./vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
  }
}
